<div class="heading">
  <h1>New Restaurant</h1>
</div>
<nav class="nav nav-tabs">
  <%= link_to "Back to restaurants", restaurants_path, class: "nav-link" %>
</nav>

<div class="container">
  <%= simple_form_for(@restaurant) do |t| %>
    <%= t.input :name %>
    <%= t.input :address %>
    <%= t.input :category %>
    <%= t.input :phone_number %>
    <%= t.submit %>
  <% end %>

</div>

